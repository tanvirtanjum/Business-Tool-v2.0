<Window x:Class="BTv2._0.FAQWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BTv2._0"
        mc:Ignorable="d"
        Title="FAQ's" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize" Height="450" Width="671.6">
    <Grid Margin="-2,0,-1.4,0">
        <Grid.Background>
            <ImageBrush ImageSource="FAQFRAME.jpg"/>
        </Grid.Background>
        <ScrollViewer VerticalScrollBarVisibility="Auto" Margin="5,22,54,63">
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="448" Width="684" FontSize="20" RenderTransformOrigin="0.5,0.5" TextAlignment="Center" FontFamily="Times New Roman">
                <TextBlock.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="-0.081"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </TextBlock.RenderTransform><Run Foreground="#FF1E14C3" FontWeight="Bold" FontSize="30" Text="Business Tool"/><LineBreak/><Run Foreground="#FFA0A0A0" FontSize="15" Text="version 2.0"/><LineBreak/><Run Foreground="#FFA0A0A0" FontSize="15"/><LineBreak/><Run Text="Developed by: "/><Run Foreground="#FFED1818" Text="Intrepid"/><LineBreak/><Run/><LineBreak/><Run Foreground="#FF0C8B1C" FontSize="25" Text="Developer Information:" TextDecorations="Underline"/><LineBreak/><Run Foreground="#FF2FE445" FontSize="25" TextDecorations="Underline"/><LineBreak/><Run Text="1. TANVIR TANJUM SHOURAV"/><LineBreak/><Run Text="Team Leader"/><LineBreak/><Run Foreground="#FF0929D3" Text="tanjumtanvir@gmail.com" TextDecorations="Underline"/><LineBreak/><LineBreak/><Run Text="2. RAHATUL MAKSUD"/><LineBreak/><Run Text="Designer and Developer"/><LineBreak/><Run Foreground="#FF0929D3" Text="yorahat209@gmail.com" TextDecorations="Underline"/><LineBreak/><Run/><LineBreak/><Run Text="3. ZISHAD HOSSAIN LIMON"/><LineBreak/><Run Text="Software Quality Tester and Debugger"/><LineBreak/><Run Foreground="#FF0929D3" Text="zishadlimon@gmail.com" TextDecorations="Underline"/><LineBreak/><Run/><LineBreak/></TextBlock>
        </ScrollViewer>
        <Button x:Name="backBTN" HorizontalAlignment="Left" Margin="614,9,0,0" VerticalAlignment="Top" Width="40" Background="{x:Null}" BorderBrush="{x:Null}" Height="40" Cursor="Hand" Click="backBTN_Click">
            <Image Height="25" Source="back_button_.png" Stretch="Fill" Width="30"/>
        </Button>
    </Grid>
</Window>
